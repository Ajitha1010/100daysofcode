def fact(n):
    if n==0:
        return 1
    else:
        return n*fact(n-1)
n=int(input())
fact(n)
print(fact(n))

def fib(n):
    a=0
    b=1 
    c=0
    if(n==0):
        return 0
    elif(n==1):
        return 1 
    else:
        for i in range(2,n+1):
            c=a+b 
            a=b 
            b=c 
        return c 
n=int(input())
fib(n)
print(fib(n))
