n=5
for i in range(1,n+1):
    for j in range(1,i+1):
        print(j,end=" ")
    print("\n")
n=5
for i in range(n):
    for j in range(i+1):
        print(i+1,end=" ")
    print("\n")
n=5
for i in range(n):
    for j in range(i,n):
        print("*",end=" ")
    print("\n")
n=5
for i in range(1,n+1):
    for j in range(i,n+1):
        print(j-i+1,end=" ")
    print("\n")
